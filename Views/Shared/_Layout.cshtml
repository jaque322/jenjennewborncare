<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - jenjennewborncare</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/jenjennewborncare.styles.css" asp-append-version="true" />
    <link href="~/css/owl/owl.theme.default.min.css" rel="stylesheet" />
    <link href="~/css/owl/owl.carousel.min.css" rel="stylesheet" />
    <link href="~/css/boxicons/boxicons.css" rel="stylesheet" />
    <link href="~/css/style.css" rel="stylesheet" />
</head>
<body data-bs-spy="scroll" data-bs-target=".navbar">
    <header>
        @*navbar*@
        <nav class="top-nav" id="home">
            <div class="container">
                <div class="row justify-content-between">
                    <div class="col-auto">
                        <p>
                            <a href="mailto:jenjennewborncare@gmail.com" class="text-white">
                                <i class='bx bxs-envelope'></i>
                                jenjennewborncare@gmail.com
                            </a>
                        </p>
                        <p>     
                            <a href="tel:8134080971" class="text-white">
                                <i class='bx bxs-phone'></i>
                                (813) 408-0971
                            </a>


                            @*<a class='bx bxs-phone-call'  href="tel:(813) 408-0971">(813) 408-0971</a>*@
                            @*<span>+1 (813) 408-0971</span>*@
                        </p>
                    </div>
                    <div class="col-auto">
                        <div class="social-links">
                            <a href="#"><i class='bx bxl-facebook-circle'></i></a>
                            <a href="#"><i class='bx bxl-twitter'></i></a>
                            <a href="#"><i class='bx bxl-instagram'></i></a>
                        </div>

                    </div>
                </div>
            </div>
        </nav>
        <nav class="navbar navbar-expand-lg navbar-light bg-white sticky-top">
            <div class="container">
                <a class="navbar-brand" href="#">JENJEN<span>.</span></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="collapse navbar-collapse" id="navbarNav">
                    <ul class="navbar-nav ms-auto">
                        <li class="nav-item">
                            <a class="nav-link active" aria-current="page" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" href="#">About</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Services" asp-action="index">Services</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link" asp-controller="Reviews" asp-action="Create">Review</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link disabled">Disabled</a>
                        </li>
                        <partial name="_LoginPartial"/>
                    </ul>
                    <a href="#" class="btn btn-brand ms-lg-3">Contact US</a>
                </div>
            </div>
        </nav>
        @RenderSection("HeaderContent", false)




        <div class="container">
            <main role="main" class="pb-3">
                @RenderBody()
            </main>
        </div>
    </header>
    @*About*@
    @RenderSection("About",false)


    @*milestone*@
    @*Services*@
    @RenderSection("Services",required:false)
    @*Portafolio*@
    @*team*@
    @RenderSection("Team",required:false)
    @RenderSection("Portafolio",required:false)
    @RenderSection("Milestone",required:false)
    @RenderSection("Reviews",required:false)

    

    <!-- Modal -->
   <div class="modal fade" id="exampleModal" tabindex="-1" aria-labelledby="exampleModalLabel" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-xl">
    <div class="modal-content">
    <div class="modal-body p-0">
    <div class="container-fluid">
    <div class="row gy-4">
    <div class="col-lg-4 col-sm-12 bg-cover"
    style="background-image: url(img/c2.jpg) min-height:300px">
    <div>
    </div>
    </div>
    <div class="col-lg-8">
    <form class="p-lg-5 col-12 row g-3">
    <div>
    <h1>Get in touch</h1>
    <p>Fell free to contact us and we will get back to you as soon as possible</p>
    </div>
    <div class="col-lg-6">
    <label for="userName" class="form-label">First name</label>
    <input type="text" class="form-control" placeholder="Jon" id="userName"
    aria-describedby="emailHelp">
    </div>
    <div class="col-lg-6">
    <label for="userName" class="form-label">Last name</label>
    <input type="text" class="form-control" placeholder="Doe" id="userName"
    aria-describedby="emailHelp">
    </div>
    <div class="col-12">
    <label for="userName" class="form-label">Email address</label>
    <input type="email" class="form-control" placeholder="Johndoe@example.com" id="userName"
    aria-describedby="emailHelp">
    </div>
    <div class="col-12">
    <label for="exampleInputEmail1" class="form-label">Enter Message</label>
    <textarea name="" placeholder="This is looking great and nice." class="form-control" id="" rows="4"></textarea>
    </div>

    <div class="col-12">
    <button type="submit" class="btn btn-brand">Send Message</button>
    </div>
    </form>
    </div>
    </div>
    </div>
    </div>

    </div>
    </div>
    </div>


    <footer>
    <div class="footer-top text-center wrapper">
    <div class="container">
    <div class="row justify-content-center">
    <div class="col-lg-6 text-center">
    <h4 class="navbar-brand">JENJENNEWBORNCARE<span class="dot">.</span></h4>
    <p>
    Best caring babies in Florida.
    </p>
    <div class="col-auto social-icons">
    <a href="#"><i class='bx bxl-facebook'></i></a>
    <a href="#"><i class='bx bxl-twitter'></i></a>
    <a href="#"><i class='bx bxl-instagram'></i></a>
    <a href="#"><i class='bx bxl-pinterest'></i></a>
    </div>
    </div>
    </div>
    </div>
    </div>
    <div class="footer-bottom text-center">
    <p class="mb-0">&copy; @DateTime.Today.Year. All rights Reserved</p>

    </div>
    </footer>
    @*<a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>*@
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/js/owl/owl.carousel.min.js"></script>
    <script src="~/js/app.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
